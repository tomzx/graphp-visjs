{
	"name": "tomzx/graphp-visjs",
	"description": "Visjs graph drawing for graphp/graph",
	"type": "library",
	"keywords": [
		"visjs",
		"graphp",
		"graph"
	],
	"homepage": "https://github.com/tomzx/graphp-visjs",
	"license": "MIT",
	"authors": [
		{
			"name": "Tom Rochette",
			"email": "tom@tomrochette.com",
			"homepage": "http://www.tomrochette.com"
		}
	],
	"support": {
		"issues": "https://github.com/tomzx/graphp-visjs/issues"
	},
	"require": {
		"php": ">=5.6.0",
		"clue/graph": "~0.8.0|~0.9.0"
	},
	"require-dev": {
		"phpunit/phpunit": "~5"
	},
	"autoload": {
